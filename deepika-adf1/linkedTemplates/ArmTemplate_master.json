{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "deepika-adf1"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"LS_sca1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sca1'"
		},
		"LS_sca2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_sca2'"
		},
		"MySql1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySql1'"
		},
		"MySql1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "database-1.c9m2ackiusdg.us-east-1.rds.amazonaws.com"
		},
		"MySql1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "titanic_db"
		},
		"MySql1_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "admin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "deepika-adf1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"LS_sca1_connectionString": {
						"value": "[parameters('LS_sca1_connectionString')]"
					},
					"LS_sca2_connectionString": {
						"value": "[parameters('LS_sca2_connectionString')]"
					},
					"MySql1_password": {
						"value": "[parameters('MySql1_password')]"
					},
					"MySql1_properties_typeProperties_server": {
						"value": "[parameters('MySql1_properties_typeProperties_server')]"
					},
					"MySql1_properties_typeProperties_database": {
						"value": "[parameters('MySql1_properties_typeProperties_database')]"
					},
					"MySql1_properties_typeProperties_username": {
						"value": "[parameters('MySql1_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}